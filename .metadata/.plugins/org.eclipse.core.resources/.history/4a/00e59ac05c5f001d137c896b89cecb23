//			    arr_2[i][j] = x;
//			    x++;
//			}
//		}
//
//		System.out.println();
//		
//		for (int o = 0; o < arr_i; o++) {
//			for (int p = 0; p < arr_j; p++) {
//				System.out.print(arr_2[o][p]+"  ");
//			}
//			System.out.println();
//
