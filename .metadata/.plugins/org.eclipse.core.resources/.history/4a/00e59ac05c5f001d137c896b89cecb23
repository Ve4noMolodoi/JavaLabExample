import java.util.Random;
import java.util.Scanner;

public class example18_04_07
{
	public static void main(String[] args) 
	{
		int arr_i = 5;
		int arr_j = 5;
		int k = 0, c = 0;
		int x = 0, y = arr_j, i = arr_j, j = 0;
		
		int l = 0;

		
		int[][] arr_2 = new int[arr_i][arr_j];
		Random rand = new Random();

		for (;k<arr_j;)
		{
			while (i>x)
			{
				if (k==y) {break;}
				arr_2[k][x] = c;
				System.out.print(c+" ");
				x++;
				c++;
			}
			
			while (j<y)
			{
				if (k==y) {break;}
				System.out.print(c+" ");
				arr_2[k][y-1] = c;
				c++;
				y--;
			}
			k++;
			y = arr_j;
			x = 0;
			
		}

		

		
//		System.out.print(x+" "+y);
