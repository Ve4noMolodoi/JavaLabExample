public class example18_06_07 
{
    public static void main(String[] args) 
    {
        char[] arr = new char[]{'F','{','|'};
        System.out.print("Code symbol: F, {, | :  ");

        for (int a=0; a < arr.length; a++)
        {
            System.out.print(Program.StaticMethod(arr)[a] + " ");
        }
    }

    static class Program
    {
        static int[] StaticMethod(char[] sArr)
        {
            int[] arg = new int[ic.length];
            for (int a=0;a<arg.length;a++){
                arg[a]=ic[a];
            }
            return arg;
        }
    }
}