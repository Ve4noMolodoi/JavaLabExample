import java.util.Scanner;
import java.time.YearMonth;

public class Lab2 
{
	public static void main(String[] args) 
	{
		Scanner scanner = new Scanner(System.in);
		
		System.out.print("Enter a number: ");
		int in_number = scanner.nextInt();
		scanner.close();

		if (in_number % 3) 
		{
			System.out.printf("0");
		}else 
		{
			System.out.print("1");
		}
	}
}